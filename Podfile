# Uncomment the next line to define a global platform for your project
platform :ios, '14.0'

def app_pods
  use_frameworks!
  inhibit_all_warnings!

# Core
  pod 'Moya/RxSwift'
  pod 'RxReachability'
  pod 'RxDataSources'
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'KeychainSwift'
  pod 'Unrealm'

# UI
  pod 'CCBottomRefreshControl'
  pod 'SnapKit'
  pod 'PanModal'
  pod 'MBProgressHUD'
  pod 'CHIPageControl/Aleppo'
  pod 'StepIndicator'
  pod 'FSPagerView'
end


target 'MonoReport' do
app_pods
deployment_target = '14.0'
end
